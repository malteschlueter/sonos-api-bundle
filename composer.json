{
    "name": "malteschlueter/sonos-api-bundle",
    "type": "symfony-bundle",
    "description": "Symfony MschlueterSonosApiBundle",
    "keywords": [
        "Sonos",
        "Api"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "symfony/config": "^4.3 || ^5",
        "symfony/dependency-injection": "^4.3 || ^5",
        "symfony/http-client": "^4.3 || ^5",
        "symfony/http-kernel": "^4.3 || ^5",
        "symfony/serializer": "^4.3 || ^5"
    },
    "autoload": {
        "psr-4": {
            "Mschlueter\\Bundle\\SonosApiBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15"
    }
}
